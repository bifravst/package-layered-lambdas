{
  "name": "@bifravst/package-layered-lambdas",
  "version": "0.0.0-development",
  "description": "Packages lambda functions and layers for AWS lambda",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/package-layered-lambdas.git"
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts src/**"
  },
  "keywords": [
    "aws",
    "lambda",
    "serverless"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/bifravst/package-layered-lambdas/issues"
  },
  "homepage": "https://github.com/bifravst/package-layered-lambdas#readme",
  "dependencies": {
    "aws-sdk": "^2.579.0",
    "chalk": "^3.0.0",
    "dependency-tree": "^7.1.0",
    "glob": "^7.1.6",
    "tmp": "0.1.0",
    "ts-loader": "^6.2.1",
    "webpack": "^4.41.2",
    "webpack-node-externals": "^1.7.2",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@bifravst/code-style": "^5.0.1",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "@types/dependency-tree": "^6.1.0",
    "@types/glob": "^7.1.1",
    "@types/node": "^13.1.0",
    "@types/tmp": "0.1.0",
    "@types/webpack": "^4.41.0",
    "@types/webpack-node-externals": "^1.7.0",
    "@types/yazl": "^2.4.2",
    "eslint": "^6.7.1",
    "husky": "^3.1.0"
  },
  "files": [
    "package-lock.json",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "release": {
    "branch": "saga",
    "remoteTags": true
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts",
      "git add"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write",
      "git add"
    ]
  }
}
