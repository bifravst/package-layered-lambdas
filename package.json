{
  "name": "@bifravst/package-layered-lambdas",
  "version": "0.0.0-development",
  "description": "Packages lambda functions and layers for AWS lambda",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/package-layered-lambdas.git"
  },
  "scripts": {
    "test": "node --unhandled-rejections=strict dist/test/e2e.js",
    "lint": "eslint --ext .js,.ts src/**",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "aws",
    "lambda",
    "serverless"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/bifravst/package-layered-lambdas/issues"
  },
  "homepage": "https://github.com/bifravst/package-layered-lambdas#readme",
  "dependencies": {
    "ansi-escapes": "^4.3.1",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "2.789.0",
    "chalk": "^4.1.0",
    "dependency-tree": "7.2.2",
    "glob": "^7.1.6",
    "snyk": "1.425.2",
    "table": "^6.0.3",
    "tmp": "^0.2.1",
    "ts-loader": "8.0.11",
    "webpack": "5.4.0",
    "webpack-node-externals": "^2.5.2",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@aws-cdk/aws-lambda": "1.72.0",
    "@aws-cdk/aws-s3": "1.72.0",
    "@aws-cdk/core": "1.72.0",
    "@bifravst/cloudformation-helpers": "4.0.2",
    "@bifravst/code-style": "8.0.134",
    "@bifravst/eslint-config-typescript": "5.0.75",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@types/aws-lambda": "8.10.64",
    "@types/dependency-tree": "7.2.0",
    "@types/glob": "7.1.3",
    "@types/node": "14.14.7",
    "@types/table": "6.0.0",
    "@types/terminal-kit": "1.28.2",
    "@types/tmp": "0.2.0",
    "@types/uuid": "8.3.0",
    "@types/webpack": "4.41.24",
    "@types/webpack-node-externals": "2.5.0",
    "@types/yazl": "2.4.2",
    "cdk": "1.72.0",
    "eslint": "7.13.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "prettier": "2.1.2"
  },
  "files": [
    "package-lock.json",
    "dist/src",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga)",
        "prerelease": true
      }
    ],
    "remoteTags": true
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "snyk": true
}
