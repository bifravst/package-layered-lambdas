{
  "name": "@bifravst/package-layered-lambdas",
  "version": "0.0.0-development",
  "description": "Packages lambda functions and layers for AWS lambda",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/package-layered-lambdas.git"
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts src/**"
  },
  "keywords": [
    "aws",
    "lambda",
    "serverless"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/bifravst/package-layered-lambdas/issues"
  },
  "homepage": "https://github.com/bifravst/package-layered-lambdas#readme",
  "dependencies": {
    "ansi-escapes": "^4.3.1",
    "aws-sdk": "^2.664.0",
    "chalk": "^4.0.0",
    "dependency-tree": "^7.2.1",
    "glob": "^7.1.6",
    "tables": "^2.2.2",
    "tmp": "^0.2.1",
    "ts-loader": "^7.0.2",
    "webpack": "^4.43.0",
    "webpack-node-externals": "^1.7.2",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@bifravst/code-style": "^7.1.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@types/dependency-tree": "^7.2.0",
    "@types/glob": "^7.1.1",
    "@types/node": "^13.11.1",
    "@types/table": "^5.0.0",
    "@types/terminal-kit": "^1.28.2",
    "@types/tmp": "0.1.0",
    "@types/webpack": "^4.41.11",
    "@types/webpack-node-externals": "^1.7.1",
    "@types/yazl": "^2.4.2",
    "eslint": "^6.8.0",
    "husky": "^4.2.5"
  },
  "files": [
    "package-lock.json",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  }
}
